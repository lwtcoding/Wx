<struts>
 <package name="qwe" extends="struts-default">
 
 
   
  <action name="*_*" class="{1}Action" method="{2}">
      <result name="success">/page/{1}/{2}.jsp</result>
      <result name="input">/page/{1}/{2}.jsp</result>
 <result type="redirect" name="redirect">${url}</result>
 
  </action>
 </package>
</struts>    
